<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<kwo-interface>
<fieldset>

<notebook>
<page>
 <form>
  <hbox>
   <vbox>
    <elem field="name" />
    <hbox>
     <vbox><elem field="code" /></vbox>
     <vbox><elem field="app" /></vbox>
     <vbox><elem field="status" /></vbox>
     <vbox><elem field="locale" /></vbox>
    </hbox>
   </vbox>
   <vbox>
    <elem type="relations" model="slide" label="slides" />
