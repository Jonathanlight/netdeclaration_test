<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<kwo-interface>
<fieldset>

<notebook>
<page>
 <form>
  <hbox>
   <vbox>
    <elem field="name" required="true" />
    <elem field="title" />
    <elem field="image" />
    <elem field="status" />
    <elem field="url" />
   </vbox>
   <vbox>
    <elem field="description" rows="6" />
   </vbox>
  </hbox>
  <launcher/>
 </form>
</page>
<page>
 <hbox>
  <vbox width="150px">
   <box label="actions">
    <link icon="details" />
   </box>
  </vbox>
  <vbox><deck /></vbox>
 </hbox> 
</page>
</notebook>

</fieldset>
</kwo-interface>
