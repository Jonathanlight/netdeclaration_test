<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<kwo-interface>
<fieldset label="$fieldset_label">

<notebook>

<page label="définition">
 <form>
  <elem field="name" required="true" />
  <elem field="title" />
  <elem field="content" mode="rich" rows="22" />
  <launcher/>
 </form>
</page>

<page label="opérations" built="$is_loaded">
 <hbox>
  <vbox width="20%">
   <box label="actions">
    <link icon="details" />
   </box>
  </vbox>
  <vbox><deck /></vbox>
 </hbox> 
</page>

</notebook>

</fieldset>
</kwo-interface>
