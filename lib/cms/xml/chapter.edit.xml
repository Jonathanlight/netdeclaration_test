<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<kwo-interface>
<fieldset label="$fieldset_label">
<notebook>

 <page label="définition">
  <form>
   <elem field="article_id" type="hidden" />
   <hbox>
    <vbox>
     <elem field="name" />
     <elem field="content" mode="rich" rows="20" />
    </vbox>
    <vbox>
     <elem field="position" />
    </vbox>
   </hbox>
   <launcher/>
  </form>
 </page>

 <page label="opérations" built="$is_loaded">
  <hbox>
   <vbox width="30%">
    <box label="actions">
     <link icon="details" />
    </box>
   </vbox>
   <vbox><deck /></vbox>
  </hbox> 
 </page>

</notebook>
</fieldset>
</kwo-interface>
