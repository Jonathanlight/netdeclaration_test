<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<kwo-interface>
<fieldset label="$fieldset_label">

<notebook>

<page label="définition">
 <form>
  <hbox>
   <vbox>
    <elem field="code" />
    <hbox>
     <vbox><elem field="name" /></vbox>
     <vbox><elem field="app" /></vbox>
     <vbox><elem field="status" /></vbox>
     <vbox><elem field="locales" /></vbox>
    </hbox>
    <hbox>
     <vbox width="66%"><elem field="flags" cols="2" /></vbox>
     <vbox><elem field="position" /></vbox>
    </hbox>
   </vbox>
   <vbox localized="true">
    <elem field="title" label="titre" />
    <elem field="description" rows="4" label="description" />
   </vbox>
  </hbox>
  <launcher/>
 </form>
</page>

<page label="opérations" built="$is_loaded">
 <hbox>
  <vbox width="10%">
   <box label="actions">
    <link icon="details" />
    <link icon="note" />
   </box>
  </vbox>
  <vbox><deck /></vbox>
 </hbox> 
</page>

</notebook>
</fieldset>

</kwo-interface>
