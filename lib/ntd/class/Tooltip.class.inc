  public static function getTooltip($tooltip_code){
    $tooltip =  self::$tooltips[$tooltip_code]; 
    $tooltip_html = '<div id="tooltip_'.$tooltip_code.'" style="display: none;" class="kwo-tooltip">'.$tooltip.'</div>';
    $tooltip_html .= '<img class="tooltip-info" src="/app/ntd/pix/info.png" onmouseout="Kwo.Tooltip.hide(this, \'tooltip_'.$tooltip_code.'\')" 
                      onmouseover="Kwo.Tooltip.show(this, \'tooltip_'.$tooltip_code.'\')" href="javascript:void(0)">';
    return $tooltip_html;
  }
  
