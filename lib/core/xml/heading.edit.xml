<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<kwo-interface>
<fieldset label="$fieldset_label">
<notebook>

 <page label="définition">
  <form>
   <hbox>
    <vbox>
     <elem field="name" label="nom" />
     <hbox>
       <vbox><elem field="code" required="true" /></vbox>
       <vbox><elem field="status" /></vbox>
       <vbox><elem field="model_id" options="Heading::models()" label="type" required="true" /></vbox>
     </hbox>
     <elem field="app" />
    </vbox>
    <vbox>
     <elem field="description" rows="7" />    
    </vbox>
    <vbox>
     <elem field="position" />
     <elem field="image" /> 
    </vbox>
   </hbox>
   <launcher/>
  </form>
 </page>

 <page label="opérations" built="$is_loaded">
  <hbox>
   <vbox width="30%">
    <box label="actions">
     <link icon="details" />
    </box>
   </vbox>
   <vbox><deck /></vbox>
  </hbox> 
 </page>

</notebook>
</fieldset>
</kwo-interface>
